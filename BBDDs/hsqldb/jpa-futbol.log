/*C3*/SET SCHEMA PUBLIC
drop table if exists Competicion cascade 
drop table if exists competir cascade 
drop table if exists Entrenador cascade 
drop table if exists Equipo cascade 
drop table if exists Jugador cascade 
drop table if exists nacionalidades cascade 
drop table if exists Pais cascade 
create table Competicion (codigo varchar(5) not null, organizador varchar(10), denominacion varchar(255) not null unique, primary key (codigo))
create table competir (equipo integer not null, competicion varchar(5) not null)
create table Entrenador (id integer generated by default as identity (start with 1), nombre varchar(255), pais_iso3 CHAR(3) not null, primary key (id))
create table Equipo (entrenador_id integer unique, id integer generated by default as identity (start with 1), nombre varchar(255), pais_iso3 CHAR(3), primary key (id))
create table Jugador (altura NUMERIC(3,2), demarcacion tinyint check (demarcacion between 0 and 3), dorsal integer not null, equipo_id integer, nacimiento date, id bigint generated by default as identity (start with 1), nombre varchar(255), primary key (id))
create table nacionalidades (jugador_id bigint not null, pais_iso3 CHAR(3) not null, primary key (jugador_id, pais_iso3))
create table Pais (nombre varchar(50), iso3 CHAR(3) not null, primary key (iso3))
